{
    "name": "@beckn/beckn-vendure-plugin",
    "version": "1.0.0",
    "description": "Plugin that allows a Vendure store/marketplace to get onto Beckn network",
    "license": "MIT",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "watch": "tsc -p ./tsconfig.build.json --watch",
