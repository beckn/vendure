    "devDependencies": {
        "@types/json-diff": "1.0.3",
        "@vendure/common": "^2.1.5",
        "@vendure/core": "^2.1.5",
        "@vendure/testing": "^2.1.5",
        "msw": "^2.2.1",
        "rimraf": "^3.0.2",
        "typescript": "4.9.5"
    }
